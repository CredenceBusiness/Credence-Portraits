							<script>
								function addAriaChecked6312575000001261010 (){
									var checkboxElem = event.target;
									if ( checkboxElem != undefined && checkboxElem.checked ){
										checkboxElem.ariaChecked = 'true';
									}
									else if ( checkboxElem != undefined && !checkboxElem.checked ){
										checkboxElem.ariaChecked = 'false';
									}
								}
								function privacyAlert6312575000001261010 (){
									var privacyTool = document.getElementById ( 'privacyTool6312575000001261010' );
									var privacyErr = document.getElementById ( 'privacyErr6312575000001261010' );
									if ( privacyTool != undefined && !privacyTool.checked ){
										privacyErr.style.visibility = 'visible';
										privacyTool.ariaInvalid = 'true';
										privacyTool.focus ();
										return false;
									}
									return true;
								}
								function disableErr6312575000001261010 (){
									var privacyTool = document.getElementById ( 'privacyTool6312575000001261010' );
									var privacyErr = document.getElementById ( 'privacyErr6312575000001261010' );
									if ( privacyTool != undefined && privacyTool.checked && privacyErr != undefined ){
										privacyErr.style.visibility = 'hidden';
										privacyTool.ariaInvalid = 'false';
									}
								}
								function validateEmail6312575000001261010 (){
									var form = document.forms[ 'WebToContacts6312575000001261010' ];
									var emailFld = form.querySelectorAll ( '[ftype=email]' );
									var i;
									for ( i = 0; i < emailFld.length; i++ ){
										var emailVal = emailFld[ i ].value;
										if ((emailVal.replace( /^\s+|\s+$/g,'' )).length != 0 ){
											var atpos = emailVal.indexOf ( '@' );
											var dotpos = emailVal.lastIndexOf ( '.' );
											if ( atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= emailVal.length ){
												alert ( 'Please enter a valid email address. ' );
												emailFld[ i ].focus ();
												return false;
											}
										}
									}
									return true;
								}
								function checkMandatory6312575000001261010 (){
									var mndFileds = new Array ( 'First Name', 'Last Name', 'Account Name', 'Email' );
									var fldLangVal = new Array ( 'Candidate\x20Name', 'Parent\x20Name,\x20if\x20minor', 'Photo\x20Number', 'Email' );
									for ( i = 0; i < mndFileds.length; i++ ){
										var fieldObj = document.forms[ 'WebToContacts6312575000001261010' ][ mndFileds[ i ]];
										if ( fieldObj ){
											if (((fieldObj.value ).replace( /^\s+|\s+$/g,'' )).length == 0 ){
												if ( fieldObj.type == 'file' ){
													alert ( 'Please select a file to upload.' );
													fieldObj.focus ();
													return false;
												}
												alert ( fldLangVal[ i ] + ' cannot be empty.' );
												fieldObj.focus ();
												return false;
											}
											else if ( fieldObj.nodeName == 'SELECT' ){
												if ( fieldObj.options[fieldObj.selectedIndex].value == '-None-' ){
													alert ( fldLangVal[ i ] + ' cannot be none.' );
													fieldObj.focus ();
													return false;
												}
											}
											else if ( fieldObj.type == 'checkbox' ){
												if ( fieldObj.checked == false ){
													alert ( 'Please accept  ' + fldLangVal[ i ]);
													fieldObj.focus ();
													return false;
												}
											}
											try {
												if ( fieldObj.name == 'Last Name' ){
													name = fieldObj.value;
												}
											}
											catch ( e ){}
										}
									}
									if ( !validateEmail6312575000001261010()){
										return false;
									}
									if ( !privacyAlert6312575000001261010()){
										return false;
									}
									var urlparams = new URLSearchParams ( window.location.search );
									if ( urlparams.has( 'service' ) && ( urlparams.get( 'service' ) === 'smarturl' )){
										var webform = document.getElementById ( 'webform6312575000001261010' );
										var service = urlparams.get ( 'service' );
										var smarturlfield = document.createElement ( 'input' );
										smarturlfield.setAttribute ( 'type', 'hidden' );
										smarturlfield.setAttribute ( 'value', service );
										smarturlfield.setAttribute ( 'name', 'service' );
										webform.appendChild ( smarturlfield );
									}
									document.querySelector ( '.crmWebToEntityForm .formsubmit' ).setAttribute ( 'disabled', true );
								}
								function tooltipShow6312575000001261010 ( el ){
									var tooltip = el.nextElementSibling;
									var tooltipDisplay = tooltip.style.display;
									if ( tooltipDisplay == 'none' ){
										var allTooltip = document.getElementsByClassName ( 'zcwf_tooltip_over' );
										for ( i = 0; i < allTooltip.length; i++ ){
											allTooltip[ i ].style.display = 'none';
										}
										tooltip.style.display = 'block';
									}
									else {
										tooltip.style.display = 'none';
									}
								}
							</script>
						</form>
